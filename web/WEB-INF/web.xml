<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.zdy.view.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginCtrServlet</servlet-name>
        <servlet-class>com.zdy.controller.loginCtrServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MainFrame</servlet-name>
        <servlet-class>com.zdy.view.MainFrame</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
     </servlet-mapping>
    <!--   -->
    <servlet-mapping>
        <servlet-name>loginCtrServlet</servlet-name>
        <url-pattern>/loginCtrServlet</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>MainFrame</servlet-name>
        <url-pattern>/MainFrame</url-pattern>
    </servlet-mapping>


</web-app>